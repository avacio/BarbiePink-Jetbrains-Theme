{
  "name": "BarbiePink",
  "author": "avacio",
  "dark": true,
  "editorScheme": "/BarbiePink.xml",
  "colors": {
    "lightPink": "#f29ad8",
    "plumPink": "#E89BCD",
    "bubblegum": "#DB61B1",
    "mulberry": "#C2559C",
    "tickleMePink": "#F28DB2",
    "magentaHaze": "#9E467F",
    "byzantium": "#75335e",
    "purple": "#582876",
    "pale": "#feeaf5",
    "white": "#FFFFFF",
    "mauve": "#7A6582",
    "black": "#000000",
    "skyBlue": "#51ABCF",
    "popBlue": "#60caf4"
  },
  "ui": {
    "*": {
      "background": "magentaHaze",
      "foreground": "pale",
      "infoForeground": "pale",
      "successForeground": "pale",

      "selectionBackground": "plumPink",
      "selectionForeground": "purple",
      "selectionInactiveBackground": "byzantium",
      "selectionBackgroundInactive": "#200010",

      "disabledForeground": "mauve",
      "disabledBackground": "#280015",
      "inactiveBackground": "byzantium",
      "errorForeground": "#cd860b",

      "disabledText": "#646464",
      "Borders.ContrastBorderColor" : "mauve",
      "borderColor": "#311f39",
      "disabledBorderColor": "#271d2b",
      "focusColor" : "#5a2e6f",
      "focusedBorderColor": "#5a356d",

      "separatorColor": "#4b2665"
    },
    "Button.background": "mulberry",
    "Panel.background": "mulberry",
    "Tree": {
      "hoverBackground": "#71468f",
      "hoverInactiveBackground": "#71468f"
    },
    "EditorTabs": {
      "selectedForeground": "#eceff1",
      "underlinedTabBackground": "#5c306f",
      "background": "#3e1c4c",
      "underlineColor": "#8b5c8e",
      "hoverMaskColor": "#71468f",
      "inactiveMaskColor": "#34244aff"
    },
    "ActionButton": {
      "hoverBackground": "#512663",
      "pressedBackground": "#402a4b",
      "hoverBorderColor": "#382447",
      "pressedBorderColor": "#382140"
    },
    "NotificationsToolwindow.newNotification.background": "skyBlue",
    "NotificationsToolwindow.newNotification.hoverBackground": "popBlue",
    "NotificationsToolwindow.Notification.hoverBackground": "mulberry",

    "PopupMenu.translucentBackground": "#5d3b6c",

    "MenuBar.borderColor": "#392540",
    "Menu.borderColor": "#2e2134",
    "NavBar.borderColor": "#35243d",
    "Button": {
      "startBackground": "#643578",
      "endBackground": "#643578",
      "startBorderColor": "#2f1f39",
      "endBorderColor": "#2f1f39",
      "shadowColor": "#39294c",
      "focusedBorderColor": "#352040",
      "default.focusColor": "#2c1d32",
      "disabledBorderColor": "#25192f",
      "default": {
        "startBackground": "#643578",
        "endBackground": "#643578",
        "startBorderColor": "#2f1f39",
        "endBorderColor": "#2f1f39",
        "focusedBorderColor": "#352040",
        "focusColor": "#352040"
      }
    }
